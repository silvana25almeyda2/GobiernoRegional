	<subDataset name="DetalleDoc" uuid="61acbba3-ce3a-4e29-9f2c-ab04b23a8606">
		<parameter name="id" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT        CAJA_CPT.DESCRIPCION AS CPT, CAJA_DOCUMENTO_DETALLE.CANTIDAD,
						 left(CAJA_DOCUMENTO_DETALLE.PRECIO,len(CAJA_DOCUMENTO_DETALLE.PRECIO)) as PRECIO,
                         CAJA_DOCUMENTO_DETALLE.id_documento
FROM            CAJA_PRECIOS INNER JOIN
                         CAJA_CPT ON CAJA_PRECIOS.ID_CPT = CAJA_CPT.ID_CPT INNER JOIN
                         CAJA_DOCUMENTO_DETALLE ON CAJA_PRECIOS.ID_PRECIO = CAJA_DOCUMENTO_DETALLE.ID_PRECIO
						 where CAJA_DOCUMENTO_DETALLE.ESTADO ='A' and
						 id_documento=$P{id}]]>
		</queryString>
		<field name="CPT" class="java.lang.String"/>
		<field name="CANTIDAD" class="java.lang.Integer"/>
		<field name="PRECIO" class="java.lang.String"/>
		<field name="id_documento" class="java.lang.Long"/>
	</subDataset>
	<parameter name="doc" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT      CDC.SERIE as serie,CDC.CORRELATIVO AS correlativo, CJ.DESCIPCION,

			CASE TIPO_DOCUMENTO
			WHEN '6 RUC' THEN AHC.dni+'   '+RAZON_SOCIAL
			ELSE APELLIDO_PATERNO+'  '+APELLIDO_MATERNO+' '+NOMBRES END AS 'CLIENTE' ,
			   left(CDC.total_doc,len(CDC.total_doc)) AS TOTAL,CDC.FECHA,
               CDC.HORA,AHC.dni,CDC.USUARIO,AHC.ID_CLIENTE,
			   UPPER(UE.UE_DESC) AS 'DESCRIP', UPPER(UE.DIRECCION) AS 'DIRECCION', UE.RUC,RTRIM(UE.TELEFONO) AS TELEFONO , SD.nombre_distrito,
               SP.nombre_provincia, SDEP.nombre_departamento,
			   PP.ape_pat_per+' '+PP.ape_mat_per+' '+PP.nombres_per AS PERSONAL
FROM  CAJA_CLIENTES AHC,
CAJA_DOCUMENTO_CABECERA CDC,
CAJA_FORMA_PAGO CJ ,
SISTEMA_USUARIO U,
PERSONAL PP,
SISTEMA_UNIDAD_EJECUTORA UE,
SISTEMA_DISTRITO SD,
SISTEMA_PROVINCIA SP,
SISTEMA_DEPARTAMENTO SDEP
WHERE
AHC.ID_CLIENTE = CDC.ID_CLIENTE AND
CJ.ID_FORMA_PAGO=CDC.ID_FORMA_PAGO AND
U.Usu_Usuario=CDC.USUARIO AND
PP.cod_per=U.cod_per AND
UE.cod_dis = SD.cod_dis  AND
SD.cod_prov = SP.cod_prov AND
SP.cod_dep = SDEP.cod_dep

AND  CDC.id_documento=$P{doc}]]>
	</queryString>
